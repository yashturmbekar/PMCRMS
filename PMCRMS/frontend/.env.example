# Local Development - Copy this file to .env.local and customize as needed

# API Configuration
VITE_API_BASE_URL=http://localhost:5086
VITE_API_TIMEOUT=30000
VITE_FILE_UPLOAD_TIMEOUT=300000

# UI Timeout Configuration (in milliseconds)
VITE_SUCCESS_MESSAGE_TIMEOUT=3000
VITE_ERROR_MESSAGE_TIMEOUT=5000
VITE_EXTENDED_ERROR_TIMEOUT=7000
VITE_REDIRECT_DELAY=1500

# Notification Configuration
VITE_NOTIFICATION_POLL_INTERVAL=30000
VITE_MAX_POLL_ATTEMPTS=40

# Admin Configuration (comma-separated email addresses)
VITE_ADMIN_EMAILS=admin@example.com,admin2@example.com
